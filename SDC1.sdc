#**************************************************************
# Time Information
#**************************************************************

set_time_format -unit ns -decimal_places 3



#**************************************************************
# Create Clock
#**************************************************************

create_clock -name {CLOCK} -period 20.000 -waveform { 0.000 10.000 } [get_ports {CLOCK}]

#**************************************************************
# Create Generated Clock
#**************************************************************
derive_pll_clocks